<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane minHeight="400.0" minWidth="600.0" styleClass="root" stylesheets="@JMetroLightTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="comandaeletronica.HomeController">
   <children>
      <BorderPane fx:id="principal">
         <left>
            <VBox fx:id="vbSide" alignment="TOP_CENTER" prefWidth="200.0" styleClass="left" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fx:id="imgMenu" fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </ImageView>
                  <TextField fx:id="tfProcurar" promptText="Procurar">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></TextField>
                  <ListView fx:id="lvMenu" maxHeight="220.0" onMouseClicked="#TrocaTela" />
               </children>
            </VBox>
         </left>
         <center>
              <fx:include source="Inicio.fxml" />
         </center>
      </BorderPane>
   </children>
</StackPane>
